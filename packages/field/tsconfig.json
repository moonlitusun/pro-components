{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@dz-web/antd-pro-provider": ["../../packages/provider/src/index.tsx"],
      "@dz-web/antd-pro-utils": ["../../packages/utils/src/index.tsx"]
    }
  },
  "include": ["./src"]
}
